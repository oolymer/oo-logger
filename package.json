{
  "name": "oo-logger",
  "description": "Simple logger",
  "version": "0.0.0",
  "author": "Benjamin Gudehus <benjamin.gudehus@canoo.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/hastebrot/canoo-route-elements.git"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "browser": "./node_modules/.bin/npm-run-all --parallel serve watch",
    "build": "./node_modules/.bin/polymer build",
    "build:docs": "./node_modules/.bin/polymer analyze > analysis.json",
    "lint": "npm run lint:polymer",
    "lint:js": "./node_modules/.bin/eslint . --ext js,html --ignore-path .gitignore",
    "lint:polymer": "./node_modules/.bin/polymer lint",
    "serve": "./node_modules/.bin/polymer serve --port 9090",
    "test": "./node_modules/.bin/polymer test --local firefox",
    "watch": "./node_modules/.bin/browser-sync start --port 3000 --proxy 'localhost:9090' --no-ui --files '*.html, *.js, demo/**/*.html, test/**/*.html' --startPath \"/components/$npm_package_name/\""
  },
  "devDependencies": {
    "bower": "1.8.2",
    "browser-sync": "2.18.13",
    "npm-run-all": "4.1.2",
    "polymer-cli": "1.5.7",
    "web-component-tester": "6.4.1"
  }
}
